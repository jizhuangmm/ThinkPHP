                                        function del(id,obj)
                                        {
                                            var result=confirm("您确定要删除吗?");
                                            if(result)
                                            {
                                                $.ajax({
                                                url:"__URL__/del_modelid",
                                                type:"post",
                                                async:false,
                                                data:{"id":id},
                                                    success:function(e)
                                                    {
                                                        if(e==1)
                                                        {
                                                            obj.parentNode.parentNode.remove();
                                                        }
                                                    }
                                                }) 
                                            }   

                                        }

                                        function is_show(id,is_show)
                                        {
                                          $.ajax({
                                              url:"__URL__/nav_is_show",
                                              type:"post",
                                              async:false,
                                              data:{"id":id,"is_show":is_show},
                                              success:function(e)
                                              {
                                                  if(e==1)
                                                  {
                                                      location=location;
                                                  }
                                              }
                                              })
                                        }

