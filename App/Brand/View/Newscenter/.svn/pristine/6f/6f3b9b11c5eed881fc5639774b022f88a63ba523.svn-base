                           <a href="__URL__/show_model_info?id={$vo.id}">{$vo.model_name}</a>
                           <span class="divider">/</span>
                       </li>
                      </volist>
                       <li class="pull-right search-wrap">
                           <form action="__URL__/show_model_info" class="hidden-phone" method="post">
